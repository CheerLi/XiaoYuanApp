<resources>

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light"></style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="BackTheme" parent="AppBaseTheme">
        <item name="android:homeAsUpIndicator">@drawable/nav_back</item>
    </style>

    <style name="actionbar_def_style" parent="@android:style/Widget.Holo.Light.ActionBar"></style>

    <style name="ActivityTheme_Light" parent="android:Theme.Holo.Light">
        <item name="android:actionBarStyle">@style/actionbar_def_style</item>
    </style>

</resources>